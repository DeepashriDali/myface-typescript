<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Posts</h1>
<ol>
    <% results.forEach(function(post) {%>
        <li>
            <img src=<%= post.imageUrl %> alt = "Post Image" height = "150" width = "150" />
           <p><%= post.message %></p> 
           <p>Posted By: <%= post.postedBy.name %> on <%= post.createdAt %></p> 
        </li>
    <% }); %>
</ol>
</body>
<%- include('_footer') -%>
</html>